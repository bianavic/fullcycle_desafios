# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Order {
  id: String!
  price: Float!
  tax: Float!
  finalPrice: Float!
}

input OrderInput {
  id: String!
  price: Float!
  tax: Float!
}

type Query {
  listOrders: [Order!]!
}

type Mutation {
  createOrder(input: OrderInput): Order
}
